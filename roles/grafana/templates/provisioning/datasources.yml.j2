apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    orgId: 1
    url: "http://{{ grafana_prometheus_host }}:{{ prometheus_port }}"
    isDefault: true
    version: 1
    editable: false

  - name: Loki
    type: loki
    access: proxy
    url: "http://{{ grafana_loki_host }}:{{ loki_port }}"
    isDefault: false
    editable: true
